{
  "name": "getgot-mobile-template",
  "version": "1.0.0",
  "private": true,
  "homepage": "http://ec2-35-170-186-135.compute-1.amazonaws.com/login/",
  "dependencies": {
    "@babel/polyfill": "7.6.0",
    "antd": "3.23.6",
    "classnames": "2.2.6",
    "core-js": "3.2.1",
    "fp-ts": "2.1.0",
    "fp-ts-contrib": "0.1.5",
    "io-ts": "2.0.1",
    "io-ts-reporters": "1.0.0",
    "io-ts-types": "0.5.1",
    "json5": "2.1.1",
    "query-string": "6.8.3",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-helmet": "5.2.1",
    "react-scripts": "3.2.0",
    "react-transition-group": "4.3.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "dev": "concurrently \"npm:start\"  \"npm:watch-*\"",
    "watch-css": "tcm --watch -p src/**/*.module.css",
    "watch-scss": "tsm --watch src/**/*.module.scss",
    "start": "react-app-rewired --max_old_space_size=4096 start",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "//": "Deployment concerns",
    "deploy": "npm run zip && npm run tag",
    "zip": "cd build && zip -r -X ../GetGotMobileTemplate_$(date -u +\"%Y-%m-%dT%H-%M-%SZ\").zip .",
    "tag": "git tag -a \"GetGotMobileTemplate_$(date -u +\"%Y-%m-%dT%H-%M-%SZ\")\" -m 'Generated build for GetGotMobileTemplate_$(date -u +\"%Y-%m-%dT%H:%M:%SZ\") on $(hostname -s)' HEAD"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/json5": "0.0.30",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-helmet": "5.0.11",
    "@types/react-transition-group": "4.2.3",
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "@typescript-eslint/parser": "2.3.3",
    "babel-eslint": "10.0.3",
    "babel-plugin-import": "1.12.2",
    "concurrently": "5.0.0",
    "customize-cra": "0.8.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-config-problems": "3.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.1.2",
    "html-loader": "0.5.5",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "typed-css-modules": "0.6.2",
    "typed-scss-modules": "0.0.12",
    "utility-types": "3.8.0"
  }
}
