    "@types/react-redux": "7.1.9",
    "@types/react-transition-group": "4.4.0",
    "@types/redux-first-router": "2.1.5",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.0",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "customize-cra": "0.9.1",
