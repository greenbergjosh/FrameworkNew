    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-redux": "7.2.6",
    "react-scripts": "5.0.0-next.60",
    "redux": "4.1.2",
    "styled-components": "5.3.3",
    "uuid": "8.3.2",
    "web-vitals": "2.1.3"
  },
  "scripts": {
    "start": "concurrently \"npm:start-rs\"  \"npm:watch-*\"",
    "start-win": "yarn start",
    "watch-css": "tcm --watch -p src/**/*.module.css",
    "watch-scss": "tsm --watch src/**/*.module.scss",
    "start-rs": "craco start --openssl-legacy-provider --max_old_space_size=4096",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint './src/**/*.{ts,tsx}' --fix",
    "//": "Deployment concerns",
    "deploy": "npm run zip && npm run tag",
    "zip": "cd build && zip -r -X ../ChannelAdmin_$(date -u +\"%Y-%m-%dT%H-%M-%SZ\").zip .",
    "tag": "git tag -a \"ChannelAdmin_$(date -u +\"%Y-%m-%dT%H-%M-%SZ\")\" -m 'Generated build for ChannelAdmin_$(date -u +\"%Y-%m-%dT%H:%M:%SZ\") on $(hostname -s)' HEAD"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "2.3.1",
    "@types/flexsearch": "0.7.2",
    "@types/gapi.auth2": "0.0.56",
    "@types/jest": "27.4.0",
    "@types/json5": "2.2.0",
    "@types/loadable__component": "5.13.4",
