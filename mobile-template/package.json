{
  "name": "getgot-mobile-template",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "antd": "^3.20.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "core-js": "^2.6.5",
    "font-awesome": "^4.7.0",
    "formik": "^1.5.4",
    "fp-ts": "^1.17.1",
    "fp-ts-contrib": "0.0.3",
    "io-ts": "^1.8.5",
    "io-ts-reporters": "0.0.21",
    "io-ts-types": "^0.4.7",
    "json5": "^2.1.0",
    "lodash.debounce": "^4.0.8",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-player": "^1.13.0",
    "react-scripts": "3.0.0",
    "react-transition-group": "^4.0.0",
    "typescript": "3.4.5",
    "valid-url": "^1.0.9"
  },
  "scripts": {
    "dev": "concurrently \"npm:start\"  \"npm:watch-*\"",
    "watch-css": "tcm --watch -p src/**/*.module.css",
    "watch-scss": "tsm --watch src/**/*.module.scss",
    "start": "react-app-rewired --max_old_space_size=4096 start",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "//": "Deployment concerns",
    "deploy": "npm run zip && npm run tag",
    "zip": "cd build && zip -r -X ../GetGotMobileTemplate_$(date -u +\"%Y-%m-%dT%H-%M-%SZ\").zip .",
    "tag": "git tag -a \"GetGotMobileTemplate_$(date -u +\"%Y-%m-%dT%H-%M-%SZ\")\" -m 'Generated build for GetGotMobileTemplate_$(date -u +\"%Y-%m-%dT%H:%M:%SZ\") on $(hostname -s)' HEAD"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/json5": "0.0.30",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "11.13.7",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-transition-group": "^2.9.1",
    "@types/valid-url": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "concurrently": "^4.1.0",
    "customize-cra": "^0.2.12",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-problems": "^2.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-loader": "^0.5.5",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "react-app-rewired": "^2.1.3",
    "typed-css-modules": "^0.4.2",
    "typed-scss-modules": "0.0.10",
    "utility-types": "^3.5.0"
  }
}
