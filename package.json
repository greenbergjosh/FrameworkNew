{
  "name": "interface-builder",
  "version": "0.1.0",
  "private": true,
  "description": "React Library for creating WYSIWYG interfaces",
  "author": "OnPoint UI Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onpointglobal/interface-builder.git"
  },
  "homepage": "https://github.com/onpointglobal/interface-builder#readme",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "start": "tsc -w -p .",
    "build": "npm-run-all build:clean build:ts build:sass build:copy-nonsource build:replace-sass-imports build:replace-sass-requires",
    "build:clean": "rimraf ./lib",
    "build:ts": "tsc -p .",
    "build:sass": "node-sass ./src --output ./lib",
    "build:copy-nonsource": "copyfiles -u 1 \"src/**/*.jpg\" \"src/**/*.png\" \"src/**/*.module.css\"  ./lib",
    "build:replace-sass-imports": "replace 'import (.*?)\\.s[ac]ss(.*)' 'import $1.css$2' ./lib -r",
    "build:replace-sass-requires": "replace 'require\\((.*?).s[ac]ss(.*?)\\)' 'require($1.css$2)' ./lib -r",
    "wp-build": "webpack"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@reach/component-component": "0.1.3",
    "@syncfusion/ej2-react-grids": "17.1.42",
    "@syncfusion/ej2-react-navigations": "17.1.43",
    "antd": "3.20.1",
    "ckeditor4": "4.13.0",
    "ckeditor4-react": "1.0.0",
    "fp-ts": "1.17.1",
    "io-ts": "1.8.5",
    "io-ts-types": "0.4.7",
    "json-logic-js": "^1.2.2",
    "monaco-editor": "0.18.1",
    "react-dnd": "7.4.5",
    "react-dnd-html5-backend": "7.4.4",
    "react-dnd-touch-backend": "0.8.3",
    "react-monaco-editor": "0.32.1"
  },
  "peerDependencies": {
    "classnames": ">=2.2.6",
    "lodash.debounce": ">=4.0.8",
    "monocle-ts": ">=1.7.2",
    "newtype-ts": ">=0.2.1",
    "prop-types": ">=15.6.2",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "devDependencies": {
    "@types/ckeditor": "4.9.8",
    "@types/classnames": "2.2.8",
    "@types/json5": "0.0.30",
    "@types/loadable__component": "5.6.0",
    "@types/lodash.debounce": "4.0.6",
    "@types/node": "11.13.7",
    "@types/node-sass": "4.11.0",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.14",
    "@types/react-beautiful-dnd": "11.0.2",
    "@types/react-copy-to-clipboard": "4.2.6",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/react-redux": "7.0.8",
    "@types/react-transition-group": "2.9.1",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "concurrently": "4.1.0",
    "copyfiles": "^2.1.1",
    "cross-env": "5.1.4",
    "css-loader": "^3.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-problems": "2.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "monocle-ts": "1.7.2",
    "newtype-ts": "0.2.1",
    "node-sass": "4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.17.0",
    "prop-types": "15.6.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "replace": "^1.1.1",
    "rimraf": "3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "typed-css-modules": "0.4.2",
    "typed-scss-modules": "0.0.10",
    "typescript": "3.6.4",
    "utility-types": "3.5.0",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.10"
  }
}
