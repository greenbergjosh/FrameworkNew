@using React.AspNet
@using React.RenderFunctions
@using React.Router
@model Channel.Admin.Dashboard.Controllers.DashboardController.DashboardRootViewModel

@{
	Layout = "_DashLayout";
	var styledComponentsFunctions = new StyledComponentsFunctions();
	//var helmetFunctions = new ReactHelmetFunctions();

	var chainedFunctions = new ChainedRenderFunctions(styledComponentsFunctions);
}
@Html.ReactRouter("RootComponent", new { }/*, renderFunctions: chainedFunctions*/)
@{
	System.Diagnostics.Debug.WriteLine("styledComponentsFunctions.RenderedStyles " + styledComponentsFunctions.RenderedStyles);
	ViewBag.ServerStyles = styledComponentsFunctions.RenderedStyles;
	ViewBag.HelmetTitle = "<title>Admin Dashboard</title>"; // helmetFunctions.RenderedHelmet.GetValueOrDefault("title");
}


<script src="/dist/runtime.js"></script>
<script src="/dist/vendor.js"></script>
<script src="/dist/components.js"></script>


@Html.ReactInitJavaScript()
