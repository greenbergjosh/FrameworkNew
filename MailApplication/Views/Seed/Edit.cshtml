@model BusinessLogic.Model.Seed
@{
    ViewData["Title"] = "Edit";

}
<h2>Edit Seed</h2>
<hr />
<form action="/Seed/Update" method="post">
    <div class="container">
        <div class="form-group row">
            <div class="col-10">
                <input class="form-control" type="hidden" value="@Model.Id" id="idAccount" name="idAccount">
            </div>
        </div>

        <div class="form-group row">
            <label for="example-text-input" class="col-2 col-form-label">Name</label>
            <div class="col-10">
                <input class="form-control" type="text" value="@Model.Name" id="nameAccount" name="nameAccount">
            </div>
        </div>

        <div class="form-group row">
            <label for="example-text-input" class="col-2 col-form-label">LastName</label>
            <div class="col-10">
                <input class="form-control" type="text" value="@Model.LastName" id="lastName" name="lastName">
            </div>
        </div>

        <div class="form-group row">
            <label for="example-text-input" class="col-2 col-form-label">UserName</label>
            <div class="col-10">
                <input class="form-control" type="text" value="@Model.UserName" id="userName" name="userName">
            </div>
        </div>

        <div class="form-group row">
            <label for="example-text-input" class="col-2 col-form-label">Password</label>
            <div class="col-10">
                <input class="form-control" type="password" value="@Model.Password" id="password" name="password">
            </div>
        </div>
        <input type="submit" class="btn pull-right btn-success" value="Save" />
    </div>
</form>